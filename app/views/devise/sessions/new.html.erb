<div class="container mt-5">
  <div class="row d-flex justify-content-center">
    <div class="col-md-6">
      <div class="card px-5 py-5" id="form1">
          <h1 class="h3 mb-3 fw-normal text-center">Please sign in</h1>
          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="forms-inputs mb-4"> <%= f.email_field :email, autofocus: true, class: "form-control", id: "inputEmail", placeholder: "Email", autocomplete: "email" %>
          </div>
          <div class="forms-inputs mb-4"><%= f.password_field :password, autocomplete: "current-password", class: "form-control", id: "inputEmail", placeholder: "Password" %>
          </div>
          <div class="d-flex justify-content-between">
          <div class="checkbox mb-3">
            <label>
              <input type="checkbox" value="remember-me"> Remember me
            </label>
          </div>
            <%= link_to "Forgot your password?", new_password_path(resource_name) %>
          </div>
          <div class="mb-3"> <button class="btn btn-dark w-100" type="submit">Sign in</button> </div>
          <div style="text-align:center;">
            <p>Don't have an account ? <%= link_to "Sign up", new_registration_path(resource_name) %> </p>
          </div>
          </div>
        <%end%>
      </div>
    </div>
  </div>
</div>